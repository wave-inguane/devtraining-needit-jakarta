        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }

   //Type appropriate comment here, and begin script below
   	if (newValue == ''){
		g_form.setValue('call_back_number', '');
		return;
	}
	var ga = new GlideAjax('CatalogUserScripts');
	ga.addParam('sysparm_name', 'getUserExtendedInfo');
	ga.addParam('sysparm_userid', g_form.getValue('requested_for'));
	ga.getXML(doExtendedParse);
}
function doExtendedParse(response){
	var answer = response.responseXML.documentElement.getAttribute('answer');
	var obj = JSON.parse(answer);
	g_form.setValue("call_back_number", obj.phone);
}]]></script>
